{
  "name": "morpheus-ui",
  "version": "0.1.0-alpha",
  "private": true,
  "type": "module",
  "description": "Web3 component library for Morpheus Protocol",
  "author": "Morpheus Protocol",
  "license": "MIT",
  "scripts": {
    "build": "turbo run build",
    "build:registry": "pnpm --filter @morpheus-ui/registry build",
    "build:docs": "pnpm --filter @morpheus-ui/docs build",
    "dev": "turbo run dev",
    "dev:registry": "pnpm --filter @morpheus-ui/registry dev",
    "dev:docs": "pnpm --filter @morpheus-ui/docs dev",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md,mdx,json}\"",
    "clean": "rm -rf node_modules .turbo pnpm-lock.yaml",
    "typecheck": "turbo run typecheck"
  },
  "dependencies": {
    "turbo": "^2.3.0"
  },
  "devDependencies": {
    "@types/node": "^22.9.0",
    "prettier": "^3.3.3",
    "typescript": "^5.6.3"
  },
  "engines": {
    "node": ">=20.0.0",
    "pnpm": ">=9.12.0"
  },
  "packageManager": "pnpm@9.12.0"
}
