{
  "name": "@morpheus-ui/shared-types",
  "version": "0.1.0-alpha",
  "private": true,
  "type": "module",
  "description": "Shared TypeScript types for Morpheus UI",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "tsc",
    "typecheck": "tsc --noEmit",
    "lint": "eslint . --ext ts"
  },
  "devDependencies": {
    "typescript": "^5.6.3"
  }
}
